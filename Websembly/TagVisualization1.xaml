    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:s="http://schemas.microsoft.com/surface/2008"
    Loaded="TagVisualization1_Loaded">
    

    <s:TagVisualization.Resources>
        <ImageBrush x:Key="WindowBackground" 
                  Stretch="None" Opacity="0.6" 
                  ImageSource="C:/Users/mac3/Desktop/Websembly/Websembly/Pictures/HoltonPool.jpg"/>
    </s:TagVisualization.Resources>


    <Grid>
        <s:ScatterView x:Name="scatter">
